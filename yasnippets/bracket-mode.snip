# bracket-mode yasnippet definition file
# using ;; as split char
angle-brackets;;,,;;<${1:<text>}>$0
# -ne suffix => it does not exit brackets
angle-brackets-ne;;,,,;;<$0>
square-brackets;;[[;;[${1:<text>}]$0
square-brackets-ne;;[[[;;[$0]
parentheses;;ww;;(${1:<text>})$0
parentheses-ne;;www;;($0)
curly-braces;;xx;;\{${1:<text>}\}$0
curly-braces-ne;;xxx;;\{$0\}
